{
	"editor.detectIndentation": false,
	"editor.insertSpaces": true,
	"editor.tabSize": 2,
	"files.encoding": "utf8",
	"files.eol": "\r\n",
	"files.insertFinalNewline": true,
	"files.trimFinalNewlines": false,
	"files.trimTrailingWhitespace": true,
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"**/.classpath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/.factorypath": true
	},
	"explorerExclude.backup": {},
}
